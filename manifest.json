{
  "name": "BOJ Extended",
  "description": "백준 온라인 저지(BOJ)의 기능을 확장하여 사용합니다.",
  "version": "1.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.acmicpc.net/*"],
      "all_frames": true,
      "js": [
        "js/db.js",
        "js/config.js",
        "js/utils.js",
        "js/bg-user.js",
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": ["css/style.css"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage", "tabs"]
}
