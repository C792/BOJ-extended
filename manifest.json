{
  "name": "BOJ Extended",
  "description": "백준 온라인 저지(BOJ)를 확장된 기능과 함께 사용해보세요.",
  "version": "1.3.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.acmicpc.net/?", "https://www.acmicpc.net/*"],
      "all_frames": true,
      "css": ["css/theme-dark.css"],
      "run_at": "document_start",
      "js": ["js/config.js", "js/utils.js", "js/theme.js", "js/wide.js"]
    },
    {
      "matches": ["https://www.acmicpc.net/?", "https://www.acmicpc.net/*"],
      "all_frames": true,
      "js": [
        "js/db.js",
        "js/user.js",
        "js/status.js",
        "js/group.js",
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/common.css",
    "css/status.css",
    "css/user.css",
    "js/status-rte.js"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"]
}
